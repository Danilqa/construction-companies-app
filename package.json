{
    "name": "web-app-starter",
    "version": "0.0.1",
    "license": "-",
    "private": true,
    "scripts": {
        "dev": "yarn webpack-dev-server --config=config/webpack/webpack.config.dev.js --p --progress --mode development",
        "build": "yarn webpack --config=config/webpack/webpack.config.prod.js --p --progress --mode production",
        "lint": "tslint 'src/**/*.{ts,tsx}'",
        "test": "jest",
        "test:ci": "jest --ci --coverage"
    },
    "dependencies": {
        "react": "16.7.0",
        "react-dom": "16.7.0",
        "styled-components": "4.1.3"
    },
    "lint-staged": {
        "*.{ts,tsx}": ["yarn lint"]
    },
    "devDependencies": {
        "@types/jest": "22.2.2",
        "@types/node": "10.12.18",
        "@types/react": "16.7.21",
        "@types/react-dom": "16.0.11",
        "@eigenspace/codestyle": "1.1.10",
        "clean-webpack-plugin": "1.0.1",
        "lint-staged": "8.1.1",
        "html-webpack-plugin": "3.2.0",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "typescript": "3.3.1",
        "jest": "23.6.0",
        "ts-jest": "23.10.5",
        "webpack": "4.29.0",
        "webpack-cli": "3.2.1",
        "webpack-dev-server": "3.1.14"
    }
